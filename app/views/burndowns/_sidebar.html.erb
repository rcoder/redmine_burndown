  <h3><%= l(:label_version_plural) %></h3>
  <ul id="sprint_burndown_list">
    <% @project.shared_versions.each do |version| -%>
      <li<%= %Q[ class="selected"] if (@version && @version == version) %>><%= link_to(version.name, show_burndown_path(:project_id => @project, :id => version)) %></li>
    <% end -%>
  </ul>
